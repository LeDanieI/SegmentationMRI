{
    "dataloader": {
        "data_dir": "data/",
        "splitratio": [0.6, 0.1, 0.3],
        "normalize": false,
        "extension": ".nrrd",
        "LP_dimension": 160, 
        "S_dimension": 16,
        "rotation_angle": 10,
        "N_classes": 2
    },
        
    "trainer": {
        "batch_size": 1,
        "device": "cuda",
        "epochs": 100,
        "lr": 0.0005,
        "loss_fn": "CrossEntropyLoss"
    },

    "tester": {
        "batch_size": 1,
        "device": "cuda"
    }
}